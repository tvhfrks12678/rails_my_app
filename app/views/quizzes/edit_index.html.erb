<% provide(:title, '投稿一覧') %>
<h1><%= yield(:title) %></h1>

<div class="row">
  <div class="col-md-8">
    <ul class="">
      <% @quiz_edits.each do |quiz_edit| %>
        <li>
				<%= link_to '編集', edit_quiz_path(quiz_edit.quiz_id) %>
					<%= quiz_edit.question %>
					<%= quiz_edit.created_at %>
        </li>
      <% end %>
    </ul>
  </div>
</div>